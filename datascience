        "from sklearn.linear_model import LogisticRegression\n",
        "\n",
        "lr = LogisticRegression(penalty = 'l2',max_iter=500,C=1)\n",
        "\n",
        "lr.fit(cv_train_features,train_sentiment)\n",
        "\n",
        "lr_bow_predictions = lr.predict(cv_test_features)\n"
